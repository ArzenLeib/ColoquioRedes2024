{
"builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
],
"routes": [
    {
        "src": "/(.*)",
        "dest": "/",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE",
            "OPTIONS"
        ]
    }
]
}